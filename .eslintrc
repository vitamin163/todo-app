{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "react/jsx-props-no-spreading": 0,
    "no-console": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error", {
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "useTabs": false,
        "printWidth": 80,
        "tabWidth": 2,
        "jsxBracketSameLine": false
      }
    ]
  }
}